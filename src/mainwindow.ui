QTextEdit#textEdit QScrollBar:hover{ background-color: rgb(217, 217, 217);}
QTextEdit#textEdit QScrollBar::add-line:vertical{ width:0px; height: 0px; subcontrol-position: bottom; subcontrol-origin: margin; }
QTextEdit#textEdit QScrollBar::sub-line:vertical{ width:0px; height: 0px; subcontrol-position: top; subcontrol-origin: margin; }



NoteView QScrollBar {margin-right: 2px; background: transparent;}
NoteView QScrollBar:hover { background-color: rgb(217, 217, 217);}
